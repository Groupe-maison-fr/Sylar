{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nivo/bar": "^0.67.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dockerode": "^3.2.1",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "forever": "^3.0.4",
    "http-proxy": "^1.18.1",
    "interval-promise": "^1.4.0",
    "moment": "^2.29.1",
    "node-ssh": "^11.1.1",
    "nodemon": "^2.0.6",
    "reconnecting-websocket": "^4.4.0",
    "supervisord": "^0.1.0",
    "web-push": "^3.4.4",
    "websocket": "^1.0.33"
  },
  "scripts": {
    "start-server": "forever --spinSleepTime 1300 --minUptime 1000 -c \"nodemon --exitcrash\" src/index.mjs"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*"
    ],
    "ignoreRoot": [
      ".git"
    ],
    "watch": [
      "src/**",
      "src/index.mjs",
      "package.json"
    ],
    "ext": "*",
    "verbose": true,
    "delay": 500
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
